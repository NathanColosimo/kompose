# Frontend env vars baked into the static build
NEXT_PUBLIC_WEB_URL="https://your-production-url.com"

# Tauri updater signing (required for updater artifacts)
TAURI_SIGNING_PRIVATE_KEY="/Users/your-user/.tauri/kompose.key"
TAURI_SIGNING_PRIVATE_KEY_PASSWORD="super-secure-password"

# macOS notarization via App Store Connect API key (recommended)
APPLE_API_ISSUER="YOUR_ISSUER_ID"
APPLE_API_KEY="YOUR_KEY_ID"
APPLE_API_KEY_PATH="/Users/your-user/keys/AuthKey_XXXXXXXXXX.p8"
