    "@rn-primitives/separator": "^1.2.0",
    "@rn-primitives/slot": "^1.2.0",
    "@tanstack/react-form": "^1.28.3",
    "@tanstack/react-query": "catalog:",
    "ai": "catalog:",
    "babel-preset-expo": "~55.0.6",
    "better-auth": "catalog:",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "^55.0.0-preview.12",
