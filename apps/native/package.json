{
  "name": "native",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start --clear",
    "build": "expo export --platform ios",
    "build:prod": "mkdir -p dist && EAS_LOCAL_BUILD_SKIP_CLEANUP=1 EAS_LOCAL_BUILD_WORKINGDIR=\"$HOME/.eas-build-cache/native\" eas build -p ios --profile production --local --non-interactive --output ./dist/kompose.ipa",
    "submit:prod": "eas submit -p ios --profile production --non-interactive --path ./dist/kompose.ipa --wait",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:reset-simulator": "xcrun simctl shutdown all && xcrun simctl erase all && rm -rf ~/Library/Developer/Xcode/DerivedData && expo run:ios",
    "update:preview": "eas update --channel preview --environment preview --platform ios --auto",
    "update:prod": "eas update --channel production --environment production --platform ios --auto",
    "prebuild": "expo prebuild",
    "web": "expo start --web",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/react": "catalog:",
