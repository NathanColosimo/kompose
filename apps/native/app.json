{
  "expo": {
    "name": "Kompose",
    "slug": "kompose",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/kompose-icon.png",
    "scheme": "kompose",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nathancolosimo.kompose",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "icon": {
        "dark": "./assets/images/kompose-icon.png",
        "light": "./assets/images/kompose-icon-light.png"
      }
    },
    "android": {
      "icon": "./assets/images/kompose-icon.png",
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/kompose-icon.png",
        "monochromeImage": "./assets/images/kompose-icon.png"
      },
      "predictiveBackGestureEnabled": false,
      "package": "com.nathancolosimo.kompose"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/kompose-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/kompose-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fefefe",
          "dark": {
            "image": "./assets/images/kompose-icon.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b17e2ef6-be9c-4a74-ae7f-53525505b559"
      }
    },
    "owner": "nathancolosimo",
    "runtimeVersion": {
      "policy": "fingerprint"
    },
    "updates": {
      "url": "https://u.expo.dev/b17e2ef6-be9c-4a74-ae7f-53525505b559"
    }
  }
}
