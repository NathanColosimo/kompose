{
  "name": "kompose",
  "private": true,
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "zod": "^4.1.13",
      "@orpc/server": "^1.12.2",
      "@orpc/client": "^1.12.2",
      "@orpc/openapi": "^1.12.2",
      "@orpc/zod": "^1.12.2",
      "better-auth": "^1.4.5",
      "@better-auth/expo": "^1.4.5",
      "dotenv": "^17.2.2",
      "effect": "^3.19.6",
      "@t3-oss/env-nextjs": "^0.13.8",
      "@types/react": "^19.2.7",
      "@types/react-dom": "^19.2.3",
      "@types/node": "^24.10.1"
    }
  },
  "scripts": {
    "check": "biome check --write .",
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "db:push": "turbo -F @kompose/db db:push",
    "db:studio": "turbo -F @kompose/db db:studio",
    "db:generate": "turbo -F @kompose/db db:generate",
    "db:migrate": "turbo -F @kompose/db db:migrate",
    "db:start": "turbo -F @kompose/db db:start",
    "db:watch": "turbo -F @kompose/db db:watch",
    "db:stop": "turbo -F @kompose/db db:stop",
    "db:down": "turbo -F @kompose/db db:down"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@effect/language-service": "^0.60.0",
    "turbo": "^2.6.3",
    "ultracite": "6.3.9"
  },
  "packageManager": "bun@1.3.2"
}
