{
  "name": "kompose",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.4",
  "scripts": {
    "check": "biome check --write .",
    "type-check": "turbo type-check",
    "dev": "turbo dev",
    "build": "turbo build",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "knip": "knip",
    "db:push": "turbo -F @kompose/db db:push",
    "db:studio": "turbo -F @kompose/db db:studio",
    "db:generate": "turbo -F @kompose/db db:generate",
    "db:migrate": "turbo -F @kompose/db db:migrate",
    "db:start": "turbo -F @kompose/db db:start",
    "db:watch": "turbo -F @kompose/db db:watch",
    "db:stop": "turbo -F @kompose/db db:stop",
    "db:down": "turbo -F @kompose/db db:down"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@effect/language-service": "^0.72.0",
    "@types/bun": "^1.3.7",
    "knip": "^5.82.1",
    "turbo": "^2.8.0",
    "typescript": "catalog:",
    "ultracite": "^7.1.1"
  },
  "overrides": {
    "@tanstack/query-core": "^5.90.20"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "@better-auth/expo": "^1.4.18",
      "@orpc/client": "^1.13.4",
      "@orpc/contract": "^1.13.4",
      "@orpc/openapi": "^1.13.4",
      "@orpc/server": "^1.13.4",
      "@orpc/tanstack-query": "^1.13.4",
      "@orpc/zod": "^1.13.4",
      "@t3-oss/env-nextjs": "^0.13.10",
      "@tanstack/react-query": "^5.90.20",
      "@tanstack/react-query-devtools": "^5.91.2",
      "@types/node": "^24.10.9",
      "@types/react": "19.1.10",
      "@types/react-dom": "19.1.10",
      "better-auth": "^1.4.18",
      "dotenv": "^17.2.2",
      "drizzle-orm": "^0.45.1",
      "effect": "^3.19.15",
      "jotai": "^2.17.0",
      "jotai-family": "^1.0.1",
      "jotai-tanstack-query": "^0.11.0",
      "next": "^16.1.6",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "temporal-polyfill": "^0.3.0",
      "typescript": "^5.9.3",
      "uuidv7": "^1.1.0",
      "zod": "^4.3.6"
    }
