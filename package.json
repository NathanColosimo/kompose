{
  "name": "kompose",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.4",
  "scripts": {
    "check": "biome check --write .",
    "type-check": "turbo run type-check",
    "dev": "turbo run dev db:studio",
    "build": "turbo run build",
    "build:prod": "turbo run type-check && turbo run build:prod build:prod:desktop --filter=web --filter=native",
    "build:prod:web": "turbo run type-check --filter=web... && turbo run build:prod build:prod:desktop --filter=web",
    "build:prod:native": "turbo run type-check --filter=native... && turbo run build:prod --filter=native",
    "submit:prod": "turbo run submit:prod submit:prod:desktop --filter=web --filter=native",
    "submit:prod:web": "turbo run submit:prod submit:prod:desktop --filter=web",
    "submit:prod:desktop": "turbo run submit:prod:desktop --filter=web",
    "submit:prod:native": "turbo run submit:prod --filter=native",
    "dev:native": "turbo run dev -F native",
    "dev:web": "turbo run dev db:studio -F web -F @kompose/db",
    "knip": "knip",
    "db:push": "turbo run db:push -F @kompose/db",
    "db:studio": "turbo run db:studio -F @kompose/db",
    "db:generate": "turbo run db:generate -F @kompose/db",
    "db:migrate": "turbo run db:migrate -F @kompose/db",
    "db:start": "turbo run db:start -F @kompose/db",
    "db:watch": "turbo run db:watch -F @kompose/db",
    "db:stop": "turbo run db:stop -F @kompose/db",
    "db:down": "turbo run db:down -F @kompose/db"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@effect/language-service": "^0.75.1",
    "@types/bun": "^1.3.9",
    "knip": "^5.83.1",
    "turbo": "^2.8.5",
    "typescript": "catalog:",
    "ultracite": "^7.1.5"
  },
  "overrides": {
    "@tanstack/query-core": "^5.90.20"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "@better-auth/expo": "^1.4.18",
      "@ai-sdk/openai": "^3.0.30",
      "@ai-sdk/react": "^3.0.99",
      "@orpc/client": "^1.13.5",
      "@orpc/contract": "^1.13.5",
      "@orpc/experimental-ratelimit": "^1.13.5",
      "@orpc/openapi": "^1.13.5",
      "@orpc/server": "^1.13.5",
      "@orpc/zod": "^1.13.5",
      "@t3-oss/env-nextjs": "^0.13.10",
      "@tanstack/react-query": "^5.90.21",
      "@tanstack/react-query-devtools": "^5.91.3",
      "@types/node": "^25.2.3",
      "@types/react": "~19.2.2",
      "@types/react-dom": "~19.2.2",
      "better-auth": "^1.4.18",
      "ai": "^6.0.97",
      "dotenv": "^17.3.1",
      "drizzle-orm": "^0.45.1",
      "effect": "^3.19.18",
      "jotai": "^2.18.0",
      "jotai-family": "^1.0.1",
      "jotai-tanstack-query": "^0.11.0",
      "react-hook-form": "^7.71.2",
      "next": "^16.1.6",
      "react": "19.2.4",
      "react-dom": "19.2.4",
      "tailwindcss": "^4.2.0",
      "tailwind-merge": "^3.5.0",
      "temporal-polyfill": "^0.3.0",
      "typescript": "^5.9.3",
      "resumable-stream": "^2.2.10",
      "uuidv7": "^1.1.0",
      "zod": "^4.3.6",
      "@effect/opentelemetry": "^0.61.0",
      "@opentelemetry/api": "^1.9.0",
      "@opentelemetry/exporter-trace-otlp-http": "^0.212.0",
      "@opentelemetry/instrumentation": "^0.212.0",
      "@opentelemetry/instrumentation-fetch": "^0.212.0",
      "@opentelemetry/sdk-metrics": "^2.5.1",
      "@opentelemetry/sdk-node": "^0.212.0",
      "@opentelemetry/sdk-trace-base": "^2.5.1",
      "@opentelemetry/sdk-trace-node": "^2.5.1",
      "@opentelemetry/resources": "^2.5.1",
      "@opentelemetry/sdk-trace-web": "^2.5.1",
      "@opentelemetry/semantic-conventions": "^1.33.0",
      "@orpc/otel": "^1.13.5"
    }
  }
}
