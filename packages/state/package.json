{
  "name": "@kompose/state",
  "exports": {
    "./atoms/*": {
      "default": "./src/atoms/*.ts"
    },
    "./hooks/*": {
      "default": "./src/hooks/*.ts"
    },
    "./state-provider": {
      "default": "./src/state-provider.tsx"
    },
    "./*": {
      "default": "./src/*.ts"
    }
  },
  "type": "module",
  "scripts": {
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@kompose/config": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:"
  },
  "dependencies": {
    "@kompose/api": "workspace:*",
    "@kompose/google-cal": "workspace:*",
    "@tanstack/react-query": "catalog:",
    "better-auth": "catalog:",
    "jotai": "catalog:",
    "jotai-family": "catalog:",
    "jotai-tanstack-query": "catalog:",
    "react": "catalog:",
    "temporal-polyfill": "catalog:",
    "uuidv7": "catalog:"
  }
}
